{
  "name": "angular2-webpack-quickstart",
  "version": "1.0.1",
  "description": "A webpack starter for angular 2, based on the official angular2 docs",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "postinstall": "typings install"
  },
  "license": "MIT",
